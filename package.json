{
  "name": "with-firebase",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.14.0",
    "firebase": "^4.9.1",
    "firebase-admin": "^5.8.2",
    "next": "^6.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.2.0",
    "prop-types": "^15.6.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/', '<rootDir>/node_modules/"
    ],
    "verbose": true
  },
  "now": {
    "name": "candaddyplayminecraft",
    "alias": "candaddyplayminecraft",
    "files": [
      "credentials",
      "pages",
      "package.json",
      "server.js",
      "yarn.lock"
    ],
    "scale": {
      "sfo1": {
        "min": 1,
        "max": 1
      }
    }
  }
}
